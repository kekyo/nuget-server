{
  "name": "nuget-server",
  "description": "NuGet server on Node.js",
  "version": "0.1.0",
  "keywords": [
    "nuget",
    "server",
    "typescript",
    "cli",
    "ui"
  ],
  "author": "Kouji Matsui (@kekyo@mi.kekyo.net)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kekyo/nuget-server.git"
  },
  "homepage": "https://github.com/kekyo/nuget-server#readme",
  "readme": "README_pack.md",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nuget-server": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "images/nuget-server-120.png",
    "images/nuget-server-ss-1.png"
  ],
  "scripts": {
    "build": "npm run build:server && npm run build:ui",
    "build:server": "./gen-packageMetadata.sh && vite build",
    "build:ui": "./gen-packageMetadata.sh && BUILD_TARGET=ui vite build",
    "dev": "./gen-packageMetadata.sh && vite dev",
    "test": "npm run build && vitest run",
    "init": "npm run build && node dist/cli.js -c ./dev -d ./dev/packages --auth-init",
    "import": "npm run build && node dist/cli.js -c ./dev -d ./dev/packages --import-packages",
    "start": "npm run build && node dist/cli.js -c ./dev -d ./dev/packages",
    "dump": "npm run build && screw-up dump",
    "pack": "npm run build && screw-up pack --pack-destination artifacts/"
  },
  "dependencies": {
    "@emotion/react": ">=11.14.0",
    "@emotion/styled": ">=11.14.1",
    "@fastify/passport": ">=2.4.0",
    "@fastify/secure-session": ">=7.5.0",
    "@fastify/static": ">=7.0.4",
    "@mui/icons-material": ">=7.3.1",
    "@mui/material": ">=7.3.1",
    "adm-zip": ">=0.5.10",
    "async-primitives": ">=1.0.0",
    "commander": ">=11.0.0",
    "dayjs": ">=1.11.0",
    "fastify": ">=4.28.1",
    "passport-http": ">=0.3.0",
    "passport-local": ">=1.0.0",
    "react": ">=19.1.1",
    "react-dom": ">=19.1.1",
    "react-infinite-scroll-component": ">=6.1.0",
    "xml2js": ">=0.6.2",
    "zxcvbn": ">=4.4.2"
  },
  "devDependencies": {
    "@types/zxcvbn": ">=4.4.5",
    "@types/adm-zip": ">=0.5.0",
    "@types/fs-extra": ">=11.0.1",
    "@types/node": ">=20.0.0",
    "@types/passport-http": ">=0.3.9",
    "@types/passport-local": ">=1.0.38",
    "@types/react": ">=19.1.10",
    "@types/react-dom": ">=19.1.7",
    "@types/xml2js": ">=0.4.14",
    "@vitejs/plugin-react": ">=5.0.1",
    "prettier-max": ">=0.2.0",
    "screw-up": ">=1.3.0",
    "typescript": ">=5.0.0",
    "vite": ">=5.0.0",
    "vite-plugin-dts": ">=3.0.0",
    "vitest": ">=1.0.0"
  }
}
