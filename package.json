{
  "name": "nuget-server",
  "description": "NuGet server on Node.js",
  "version": "0.1.0",
  "keywords": [
    "nuget",
    "server",
    "typescript",
    "cli"
  ],
  "author": "Kouji Matsui (@kekyo@mi.kekyo.net)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kekyo/nuget-server.git"
  },
  "homepage": "https://github.com/kekyo/nuget-server#readme",
  "readme": "README_pack.md",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nuget-server": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README_pack.md"
  ],
  "scripts": {
    "build": "vite build",
    "test": "npm run build && vitest run",
    "dump": "npm run build && screw-up dump",
    "pack": "npm run build && screw-up pack --pack-destination artifacts/"
  },
  "dependencies": {
    "bcryptjs": ">=2.4.3",
    "commander": ">=11.0.0",
    "express": ">=4.18.0"
  },
  "devDependencies": {
    "@types/adm-zip": ">=0.5.0",
    "@types/bcryptjs": ">=2.4.3",
    "@types/express": ">=4.17.0",
    "@types/fs-extra": ">=11.0.1",
    "@types/node": ">=20.0.0",
    "@types/xml2js": ">=0.4.14",
    "adm-zip": ">=0.5.10",
    "dayjs": ">=1.11.0",
    "execa": ">=8.0.1",
    "fs-extra": ">=11.1.1",
    "screw-up": ">=1.3.0",
    "typescript": ">=5.0.0",
    "vite": ">=5.0.0",
    "vite-plugin-dts": ">=3.0.0",
    "vitest": ">=1.0.0",
    "xml2js": ">=0.6.2"
  }
}
